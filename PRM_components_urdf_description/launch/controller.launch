<launch>

<rosparam file="$(find PRM_components_urdf_description)/launch/controller.yaml" command="load"/>
<node args="rev_left_position_controller rev_right_position_controller tilt_joint_position_controller pan_joint_position_controller caster_position_controller joint_state_controller " name="controller_spawner" ns="PRM_components_urdf" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/PRM_components_urdf/joint_states"/>
</node>

</launch>